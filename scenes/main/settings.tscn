script/source = "extends Control

func _ready():
	var settings = ConfigFile.new() # Cоздать объект класса парсера конфиг-файлов
	var err = settings.load(\"res://scripts/settings.ini\")
	if err != OK: 
		print(\"opening config file failed\" + str(err))
		
	$\"/root/settings/menu/TabContainer/Графика/VBoxContainer/ScreenMode/CheckButton\".pressed = settings.get_value(\"Graphics\", \"screen_mode\")
	$\"/root/settings/menu/TabContainer/Графика/VBoxContainer/Vsync/CheckButton\".pressed = settings.get_value(\"Graphics\", \"vsync\")
	$\"/root/settings/menu/TabContainer/Графика/VBoxContainer/Resolution/OptionButton\".text = settings.get_value(\"Graphics\", \"resolution\")
	$\"/root/settings/menu/TabContainer/Графика/VBoxContainer/FXAA/CheckBox\".pressed = settings.get_value(\"Graphics\", \"fxaa\")
	$\"/root/settings/menu/TabContainer/Графика/VBoxContainer/MSAA/OptionButton\".selected = settings.get_value(\"Graphics\", \"msaa\")
	$\"/root/settings/menu/TabContainer/Управление/ScrollContainer/VBoxContainer/Section1/Sensitivity/sensitivity\".value = settings.get_value(\"Controls\", \"sensitivity\")
	$\"/root/settings/menu/TabContainer/Звук/VBoxContainer/Volume/Value\".value = settings.get_value(\"Sound\", \"master_volume\")
"

[sub_resource type="GDScript" id=1]
script/source = "extends Button
	
func _pressed():
	var settings = ConfigFile.new() # Cоздать объект класса парсера конфиг-файлов
	settings.set_value(\"Graphics\", \"screen_mode\", $\"/root/settings/menu/TabContainer/Графика/VBoxContainer/ScreenMode/CheckButton\".pressed)
	settings.set_value(\"Graphics\", \"vsync\", $\"/root/settings/menu/TabContainer/Графика/VBoxContainer/Vsync/CheckButton\".pressed)
	settings.set_value(\"Graphics\", \"resolution\", $\"/root/settings/menu/TabContainer/Графика/VBoxContainer/Resolution/OptionButton\".text)
	settings.set_value(\"Graphics\", \"fxaa\", $\"/root/settings/menu/TabContainer/Графика/VBoxContainer/FXAA/CheckBox\".pressed)
	settings.set_value(\"Graphics\", \"msaa\", $\"/root/settings/menu/TabContainer/Графика/VBoxContainer/MSAA/OptionButton\".selected)
	settings.set_value(\"Controls\", \"sensitivity\", $\"/root/settings/menu/TabContainer/Управление/ScrollContainer/VBoxContainer/Section1/Sensitivity/sensitivity\".value)
	settings.set_value(\"Sound\", \"master_volume\", $\"/root/settings/menu/TabContainer/Звук/VBoxContainer/Volume/Value\".value)
	settings.save(\"res://scripts/settings.ini\")
	
	G.to('main_menu')
"

[sub_resource type="GDScript" id=4]
script/source = "extends Control

func _ready():
	AddResolutions()

func AddResolutions():
	var current = get_viewport().get_size()
	var index = 0
	for i in G.Resolitions:
		$OptionButton.add_item(i, index)
		if G.Resolitions[i] == current:
			$OptionButton._select_int(index)
		index += 1
"

